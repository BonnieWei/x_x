@font-face {
  font-family: 'iconfont';  /* project id 1342656 */
  src: url('//at.alicdn.com/t/font_1342656_8p0sth9lnnf.eot');
  src: url('//at.alicdn.com/t/font_1342656_8p0sth9lnnf.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_1342656_8p0sth9lnnf.woff2') format('woff2'),
  url('//at.alicdn.com/t/font_1342656_8p0sth9lnnf.woff') format('woff'),
  url('//at.alicdn.com/t/font_1342656_8p0sth9lnnf.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_1342656_8p0sth9lnnf.svg#iconfont') format('svg');
}

.iconfont
    font-style: normal;
    vertical-align: baseline;
    text-align: center;
    text-transform: none;
    text-rendering: auto;
    line-height: 1;
    -webkit-font-smoothing: antialiased;
    -webkit-text-stroke-width: 0.2px;
    -moz-osx-font-smoothing: grayscale;
    display: inline-block;
    font-family: "iconfont" !important;
    &:before {
        display: block;
        font-family: "iconfont" !important;
    }

